<header class="header">
  <form id="headerForm" class="headerForm">
    {{#if login}}
      <a class="input" href="/saved-fashion">Saved <i class="fa fa-heart" aria-hidden="true"></i></a>
      <a class="input" href="/logout">Logout <i class="fa fa-sign-out" aria-hidden="true"></i></a>
    {{else}}
      <a class="input" href="/login">Login</a>
      <a class="input" href="/signup">Sign Up</a>
    {{/if}}

  </form>
</header>
<div id="navbar" class="navbar">
  <div class="containernav">
    {{>nav}}
    <p></p>
  </div>
</div>
<div class="container">
  <div class="boxs">
    {{#each menOutfits}}
      <div class="box">
        <div class="innerbox">
          <a class="a" href="#"><img src="./image/menOutfits/{{img}}" alt="fashion"></a>
        </div>
        <p>{{title}}</p>
        <p class="info"><a><i id="{{id}}" class="fa fa-heart-o" onclick="btnClickOutfits({{id}})"></i></a></p>
      </div>
    {{/each}}
  </div>
</div>
</div>

<script src="js/functions.js"></script>
