<header class="header">
  <form id="headerForm" class="headerForm" action="" method="post">
    {{#if login}}
    <a class="input" href="/saved-fashion">Saved</a>
    <a class="input" href="/logout">logout</a>
    {{else}}
    <a class="input" href="/login">Login</a>
    <a class="input" href="/signup">Sign Up</a>
    {{/if}}

  </form>
</header>

<div id="navbar" class="navbar">
  <div class="containernav">
{{>nav}}
    <img src="./image/star.png" class="star" alt="star">
</div>
</div>
<div class="container">
<div class="boxs">
  {{#each womenOutfits}}
<div class="box">
  <div class="innerbox">
    <a class="a" href="#"><img src="./image/womenOutfits/{{img}}" alt="fashion"></a>
  </div>
  <p>{{title}}</p>
  <p class="info"><a><i id="{{id}}" class="fa fa-heart-o" onclick="btnClickOutfits({{id}})"></i></a></p>

</div>
  {{/each}}
</div>
</div>
</div>

<script src="js/functions.js"></script>
